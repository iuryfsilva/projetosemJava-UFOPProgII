package principal;
import componentesCurriculares.Disciplina;
import pessoas.Aluno;

public class TesteReferencia {

	public static void main(String[] args) {
		
		Aluno iury = new Aluno("2020.1.0001", "Computacao");
		Disciplina disciplina01 = new Disciplina();
		disciplina01.setNome("CSI032");
		disciplina01.setCargaHoraria(90);
		
		iury.disciplina = disciplina01;//associação entre objetos
		System.out.println(iury.disciplina);
		
	}

}
